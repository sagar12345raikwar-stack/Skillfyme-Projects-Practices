pipeline{
    
    agent any 

    Stages {
        stage:1 ('Check the code') {
            steps { 
                git branch: 'master',url: 'git@github.com:sagar12345raikwar-stack/Skillfyme-Projects-Practices.git' 
                
          }  
        }
        stage:2 ('Build the Code'){
            steps {
                echo 'Building application...'
            }
        }
        stage:3 ('Test') {
            steps {
                echo 'It is Tesitng part of our code...'
            }
        }
        stage:4 ('Code Quality Check') 
            steps {
                echo  'It is checking code quality...'
            }       
        post{
            success{
                echo 'CI Pipeline Successful'
            }
            failure{
                echo 'CI Pipeline Failed'
            }
        }
          } 
